{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": ["babel", "react", "import", "prettier"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "forOf": true,
      "generators": true,
      "modules": true,
      "spread": true,
      "templateStrings": true,
      "jsx": true
    }
  },
  "rules": {
    "consistent-return": 0,
    "key-spacing": 0,
    "quotes": 0,
    "new-cap": 0,
    "no-multi-spaces": 0,
    "no-shadow": 0,
    "no-unused-vars": 1,
    "no-use-before-define": 0
  }
}
